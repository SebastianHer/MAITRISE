%% Analyse d'image

clc;clear all;

%% Chargement du fichier

%On charge l'image
I = imread('CerclesPropres.png');
%On l'affiche
figure('Name','Image avec 3 cercles et pas de fond','NumberTitle','off')
image(I); 

%On trace une ligne qui nous permet d'avoir un estimé de la taille de
%l'objet en tirant sur les poignées (80 pour celui en bas a droite)
Estimation_rayon = imdistline;

%% 
%On efface la ligne
delete(Estimation_rayon);
figure
image(GrayImg);
